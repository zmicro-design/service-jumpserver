export SERVICE_NAME=jumpserver

export SERVICE_JUMPSERVER_IMAGE_VERSION=v2.28.6-1.0.0

# debug
export SERVICE_JUMPSERVER_DEBUG=${SERVICE_JUMPSERVER_DEBUG:-"False"}
# logger
export SERVICE_JUMPSERVER_LOG_LEVEL=${SERVICE_JUMPSERVER_LOG_LEVEL:-"ERROR"}

# web
export SERVICE_JUMPSERVER_HTTP_PORT=${SERVICE_JUMPSERVER_HTTP_PORT:-"8080"}
export SERVICE_JUMPSERVER_SSH_PORT=${SERVICE_JUMPSERVER_SSH_PORT:-"2222"}
export SERVICE_JUMPSERVER_SECRET_KEY=${SERVICE_JUMPSERVER_SECRET_KEY}
export SERVICE_JUMPSERVER_BOOTSTRAP_TOKEN=${SERVICE_JUMPSERVER_BOOTSTRAP_TOKEN}

# database
export SERVICE_JUMPSERVER_MYSQL_HOST=${SERVICE_JUMPSERVER_MYSQL_HOST}
export SERVICE_JUMPSERVER_MYSQL_PORT=${SERVICE_JUMPSERVER_MYSQL_PORT}
export SERVICE_JUMPSERVER_MYSQL_USER=${SERVICE_JUMPSERVER_MYSQL_USER}
export SERVICE_JUMPSERVER_MYSQL_PASS=${SERVICE_JUMPSERVER_MYSQL_PASS}
export SERVICE_JUMPSERVER_MYSQL_DB_NAME=${SERVICE_JUMPSERVER_MYSQL_DB_NAME}

# cache
export SERVICE_JUMPSERVER_REDIS_HOST=${SERVICE_JUMPSERVER_REDIS_HOST}
export SERVICE_JUMPSERVER_REDIS_PORT=${SERVICE_JUMPSERVER_REDIS_PORT}
export SERVICE_JUMPSERVER_REDIS_PASSWORD=${SERVICE_JUMPSERVER_REDIS_PASSWORD}
